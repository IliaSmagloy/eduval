{
  "name": "emon-student-app",
  "version": "1.0.0",
  "private": true,
  "homepage": "/",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.10.0",
    "auth0-js": "^9.14.0",
    "auth0-lock": "^11.26.3",
    "aws-iot-device-sdk": "^2.2.6",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.2",
    "camelize": "^1.0.0",
    "chart.js": "^2.9.3",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.1",
    "dateformat": "^3.0.3",
    "flux": "^3.1.3",
    "i18next": "^19.7.0",
    "i18next-browser-languagedetector": "^4.3.1",
    "i18next-xhr-backend": "^3.2.2",
    "lodash.find": "^4.6.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-chartjs-2": "^2.10.0",
    "react-dom": "^16.13.1",
    "react-dropdown": "^1.8.0",
    "react-flags": "^0.1.18",
    "react-ga": "^2.7.0",
    "react-i18next": "^11.7.2",
    "react-modal": "^3.11.2",
    "react-qr-code": "^0.1.3",
    "react-quill": "^1.3.5",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.4.3",
    "react-select": "^3.1.0",
    "shards-react": "^1.0.0",
    "shortid": "^2.2.15"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:prod": "cross-env REACT_APP_BASENAME=/ npm run build",
    "build": "cross-env REACT_APP_BASENAME=/ react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "aws s3 sync build/ s3://emon-student --cache-control max-age=31536000,public && aws s3 cp s3://emon-student/service-worker.js s3:/emon-student/service-worker.js --metadata-directive REPLACE --cache-control max-age=0,no-cache,no-store,must-revalidate --content-type application/javascript && aws s3 cp s3://emon-student/index.html s3://emon-student/index.html --metadata-directive REPLACE --cache-control max-age=0,no-cache,no-store,must-revalidate --content-type text/html",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id E3P0RLYDB4DMJZ --paths /index.html && aws cloudfront create-invalidation --distribution-id E3P0RLYDB4DMJZ --paths /service-worker.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
